{
  "name": "react-hot-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for ReactJS project with hot code reloading",
  "scripts": {
    "start": "node server.js",
    "build": "babel ./src/server -d ./dist && webpack",
    "build-run-dev": "babel ./src/server -d ./dist && PORT=3001 DEV=true node dist/index.js",
    "build-run": "babel ./src/server -d ./dist && node ./dist/index.js && webpack",
    "build:prod": "NODE_ENV=production webpack --progress --profile --colors",
    "run": "node ./dist/index.js",
    "run-dev": "PORT=3001 DEV=true node dist/index.js",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaearon/react-hot-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "author": "Dan Abramov <dan.abramov@me.com> (http://github.com/gaearon)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gaearon/react-hot-boilerplate/issues"
  },
  "homepage": "https://github.com/gaearon/react-hot-boilerplate",
  "devDependencies": {
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.6.2",
    "express": "^4.13.4",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "body-parser": "^1.17.1",
    "css-loader": "^0.28.0",
    "express": "^4.15.2",
    "express-validator": "^3.2.0",
    "flexbox-grid-stylus": "^3.2.2",
    "knex": "^0.12.9",
    "mysql": "^2.13.0",
    "node-sass": "^4.5.2",
    "react": "^0.14.6",
    "react-bootstrap-modal": "^3.0.1",
    "react-dom": "^0.14.6",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-select": "^1.0.0-rc.3",
    "redux": "^3.6.0",
    "sass-loader": "^6.0.3",
    "sqlite3": "^3.1.8",
    "style-loader": "^0.16.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "tippy.js": "^0.5.1",
    "validator": "^7.0.0"
  }
}
